{% extends 'base.html.twig' %}

{% block main %}


<div class="main">
    <div class="main-inner">
        <div class="container">
            <div class="row">


                <div class="col-sm-12 col-lg-12">
                    <div class="content">
                        {{ form_start(form) }}
                        <div class="page-title">
                            <h1>Register</h1>
                        </div><!-- /.page-title -->

                        <div class="background-white p20 mb30">


                            <div class="row">
                                <div class="form-group col-sm-12 center">

                                    {{ form_errors(form) }}
                                    {{ form_widget(form) }}
                                    {{ form_help(form) }}

                                </div><!-- /.form-group -->

                                <div class="row">
                                    <div class="form-group col-sm-8 mt50">
                                        {{ form_row(form.submit, { 'attr': {'class': 'btn btn-primary pull-right form-control'} }) }}
                                    </div><!-- /.form-group -->
                                </div>


                            </div><!-- /.row -->
                        </div>



                        {{ form_end(form) }}

                    </div><!-- /.content -->
                </div><!-- /.col-* -->
            </div><!-- /.row -->
        </div><!-- /.container -->
    </div><!-- /.main-inner -->
</div>


{% endblock %}

